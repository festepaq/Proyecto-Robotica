    CONST robtarget izquierda_sale:=[[318.934,1164.645,810],[0,1,0,0],[0,0,1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget Arriba_izq:=[[100,925,1400],[0,0.707106781,0,0.707106781],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
