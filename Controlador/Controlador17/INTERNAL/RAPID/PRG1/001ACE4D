    PROC main()
        !Añada aquí su código
        impulso_giro_0_90;
        mover_encima;
        impulso_giro_90_180;
        
    ENDPROC
    
    PROC mover_encima()
        MoveJ Arriba_izq,v1000,z100,Vara\WObj:=Workobject_2;
        MoveL Arriba_centro,v300,z20,Vara\WObj:=Workobject_2;
        MoveL Arriba_derecha,v300,z20,Vara\WObj:=Workobject_2;
    ENDPROC
    PROC impulso_giro_0_90()
        MoveJ dereecha_sale,v1000,z100,Vara\WObj:=Workobject_2;
        MoveL dereecha,v300,fine,Vara\WObj:=Workobject_2;
        SetDO MESA_MUEVE_1_90,1;
        SetDO MESA_MUEVE_1_90,0;
        MoveC medio,izquierda,v300,z20,Vara\WObj:=Workobject_2;
        MoveL izquierda_sale,v300,fine,Vara\WObj:=Workobject_2;
    ENDPROC
    PROC impulso_giro_90_180()
        MoveJ dereecha_sale,v1000,z100,Vara\WObj:=Workobject_2;
        MoveL dereecha,v300,fine,Vara\WObj:=Workobject_2;
        SetDO MESA_MUEVE_2_90,1;
        SetDO MESA_MUEVE_2_90,0;
        MoveC medio,izquierda,v300,z20,Vara\WObj:=Workobject_2;
        MoveL izquierda_sale,v300,fine,Vara\WObj:=Workobject_2;
    ENDPROC
ENDMODULE